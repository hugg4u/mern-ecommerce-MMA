{
  "expo": {
    "name": "HelaShop",
    "slug": "HelaShop",
    "version": "1.0.0",
    "orientation": "potrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.helashop.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.helashop.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "helashop",
    "plugins": [
      "expo-linking"
    ],
    "extra": {
      "eas": {
        "projectId": "helashop-app"
      }
    }
  }
}
